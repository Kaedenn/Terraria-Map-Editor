<Window x:Class="TEditXna.Editor.Plugins.AnalyzeTilesPluginView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:view="clr-namespace:TEditXna.View" SizeToContent="WidthAndHeight"
        Title="Analyze Tiles" WindowStartupLocation="CenterOwner" Icon="/TEditXna;component/Images/tedit.ico">
    <StackPanel Background="{StaticResource ControlBackgroundBrush}" >
        <ScrollViewer MaxHeight="600">
            <TextBox Name="Results" Loaded="TextBlock_Loaded" Margin="10 2" HorizontalAlignment="Left" FontFamily="Lucida Console" VerticalAlignment="Top" Foreground="{StaticResource TextBrush}"></TextBox>
        </ScrollViewer>
        <Grid>
            <Button Margin="5" Content="Close" HorizontalAlignment="Right" Padding="20, 3" VerticalAlignment="Center" Click="CloseButtonClick" />
        </Grid>
    </StackPanel>
</Window>
